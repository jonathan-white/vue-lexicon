{
  "name": "lexicon",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.1",
    "dotenv": "^6.2.0",
    "firebase": "^5.11.1",
    "firebaseui": "^3.6.1",
    "moment": "^2.24.0",
    "vue": "^2.6.10",
    "vuefire": "^2.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "vue-template-compiler": "^2.6.10"
